{
    "deckName": "Probability Distributions",
    "groups": ["common", "more complex"],
    "options": [
      "Name",
      "Type",
      "Parameters",
      "PMF/PDF",
      "CDF",
      "E[X]",
      "Var(X)",
      "When to Use"
