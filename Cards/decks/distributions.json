    ],
    "cards": [
      {
        "group": "common",
        "Name": "Bernoulli",
        "Type": "Discrete",
        "Parameters": "p (0 < p < 1)",
        "PMF/PDF": "$P(X=1)=p, \\; P(X=0)=1-p$",
        "CDF": "$F(x) = 0 \\text{ for } x<0; \\; 1-p \\text{ for } 0\\le x<1; \\; 1 \\text{ for } x\\ge 1$",
        "E[X]": "$E[X] = p$",
        "Var(X)": "$Var(X) = p(1-p)$",
        "When to Use": "Single trial with success/failure outcome",
        "MGF": "$M_X(t) = 1-p + p e^t$",
        "Support": "{0,1}",
        "Special Notes": "Building block of Binomial",
        "Relationships": "Sum of n Bernoulli → Binomial(n,p)"
      },
      {
        "group": "common",
        "Name": "Binomial",
        "Type": "Discrete",
        "Parameters": "n, p",
        "PMF/PDF": "$P(X=k) = \\binom{n}{k} p^k (1-p)^{n-k}, \\; k=0,1,...,n$",
        "CDF": "$F(k) = \\sum_{i=0}^{k} \\binom{n}{i} p^i (1-p)^{n-i}$",
        "E[X]": "$E[X] = np$",
        "Var(X)": "$Var(X) = np(1-p)$",
        "When to Use": "Number of successes in n independent trials",
        "MGF": "$M_X(t) = (1-p + p e^t)^n$",
        "Support": "{0,...,n}",
        "Special Notes": "Sum of n Bernoulli trials",
        "Relationships": "Binomial(n,1/2) symmetric; Poisson limit for large n, small p"
      },
      {
        "group": "common",
        "Name": "Geometric",
        "Type": "Discrete",
        "Parameters": "p",
        "PMF/PDF": "$P(X=k)=(1-p)^{k-1} p, \\; k=1,2,...$",
        "CDF": "$F(k) = 1 - (1-p)^k$",
        "E[X]": "$E[X] = 1/p$",
        "Var(X)": "$Var(X) = (1-p)/p^2$",
        "When to Use": "Number of trials until first success",
        "MGF": "$M_X(t) = \\frac{p e^t}{1-(1-p)e^t}$",
        "Support": "{1,2,...}",
        "Special Notes": "Memoryless property",
        "Relationships": "Negative Binomial with r=1"
      },
      {
        "group": "common",
        "Name": "Negative Binomial",
        "Type": "Discrete",
        "Parameters": "r, p",
        "PMF/PDF": "$P(X=k) = \\binom{k+r-1}{k} p^r (1-p)^k, \\; k=0,1,...$",
        "CDF": "$F(k) = \\sum_{i=0}^k \\binom{i+r-1}{i} p^r (1-p)^i$",
        "E[X]": "$E[X] = r (1-p)/p$",
        "Var(X)": "$Var(X) = r (1-p)/p^2$",
        "When to Use": "Number of failures until r successes",
        "MGF": "$M_X(t) = \\left( \\frac{p}{1-(1-p)e^t} \\right)^r$",
        "Support": "{0,1,...}",
        "Special Notes": "Generalization of Geometric",
        "Relationships": "Sum of r independent Geometric(p)"
      },
      {
        "group": "common",
        "Name": "Poisson",
        "Type": "Discrete",
        "Parameters": "\\(\\lambda >0\\)",
        "PMF/PDF": "$P(X=k) = \\frac{\\lambda^k e^{-\\lambda}}{k!}, \\; k=0,1,...$",
        "CDF": "$F(k) = \\sum_{i=0}^{k} \\frac{\\lambda^i e^{-\\lambda}}{i!}$",
        "E[X]": "$E[X] = \\lambda$",
        "Var(X)": "$Var(X) = \\lambda$",
        "When to Use": "Number of events in fixed interval, rare events",
        "MGF": "$M_X(t) = \\exp(\\lambda (e^t-1))$",
        "Support": "{0,1,...}",
        "Special Notes": "Poisson process; limit of Binomial(n,p) for large n, small p",
        "Relationships": "Sum of independent Poissons → Poisson(sum λ)"
      },
      {
        "group": "common",
        "Name": "Hypergeometric",
        "Type": "Discrete",
        "Parameters": "N, K, n",
        "PMF/PDF": "$P(X=k) = \\frac{\\binom{K}{k} \\binom{N-K}{n-k}}{\\binom{N}{n}}$",
        "CDF": "$F(k) = \\sum_{i=0}^k P(X=i)$",
        "E[X]": "$E[X] = n K / N$",
        "Var(X)": "$Var(X) = n \\frac{K}{N} (1-\\frac{K}{N}) \\frac{N-n}{N-1}$",
        "When to Use": "Sampling without replacement",
        "MGF": "No simple form",
        "Support": "$\\max(0,n-(N-K)) \\le k \\le \\min(n,K)$",
        "Special Notes": "Finite population correction",
        "Relationships": "Binomial → Hypergeometric if N→∞"
      },
      {
        "group": "more complex",
        "Name": "Multinomial",
        "Type": "Discrete",
        "Parameters": "n, p1,...,pk; sum pi=1",
        "PMF/PDF": "$P(X_1=x_1,...,X_k=x_k) = \\frac{n!}{x_1!...x_k!} p_1^{x_1}...p_k^{x_k}$",
        "CDF": "No simple closed form",
        "E[X]": "$E[X_i] = n p_i$",
        "Var(X)": "$Var(X_i) = n p_i (1-p_i)$",
        "When to Use": "n independent trials with k possible outcomes",
        "MGF": "$M_X(t_1,...,t_k) = \\prod_{i=1}^k (p_i e^{t_i} + 1-p_i)^n$",
        "Support": "$x_i \\ge 0, \\sum x_i = n$",
        "Special Notes": "Generalizes Binomial",
        "Relationships": "Marginals are Binomial"
      },
      {
        "group": "more complex",
        "Name": "Multivariate Hypergeometric",
        "Type": "Discrete",
        "Parameters": "N1,...,Nk, n",
        "PMF/PDF": "$P(X_1=x_1,...,X_k=x_k) = \\frac{\\prod_{i=1}^k \\binom{N_i}{x_i}}{\\binom{N}{n}}$",
        "CDF": "No simple closed form",
        "E[X]": "$E[X_i] = n N_i / N$",
        "Var(X)": "$Var(X_i) = n (N_i/N) (1-N_i/N) (N-n)/(N-1)$",
        "When to Use": "Sampling without replacement from multiple categories",
        "MGF": "No simple form",
        "Support": "$x_i \\ge 0, \\sum x_i = n$",
        "Special Notes": "Generalization of Hypergeometric",
        "Relationships": "Approaches Multinomial when N large"
      },
      {
        "group": "common",
        "Name": "Discrete Uniform",
        "Type": "Discrete",
        "Parameters": "a,b integers, a≤X≤b",
        "PMF/PDF": "$P(X=x)=1/(b-a+1)$",
        "CDF": "$F(x) = (x-a+1)/(b-a+1), a\\le x\\le b$",
        "E[X]": "$E[X] = (a+b)/2$",
        "Var(X)": "$Var(X) = ((b-a+1)^2 -1)/12$",
        "When to Use": "All outcomes equally likely",
        "MGF": "$M_X(t) = \\frac{e^{t a} - e^{t (b+1)}}{(b-a+1)(1-e^t)}$",
        "Support": "{a,...,b}",
        "Special Notes": "Simplest discrete distribution",
        "Relationships": "Limit of Binomial with p=1/n"
      },
      {
        "group": "common",
        "Name": "Uniform",
        "Type": "Continuous",
        "Parameters": "a < b",
        "PMF/PDF": "$f(x) = 1/(b-a), a\\le x\\le b$",
        "CDF": "$F(x) = (x-a)/(b-a), a\\le x\\le b$",
        "E[X]": "$E[X] = (a+b)/2$",
        "Var(X)": "$Var(X) = (b-a)^2/12$",
        "When to Use": "All continuous outcomes equally likely",
        "MGF": "$M_X(t) = (e^{tb}-e^{ta})/(t(b-a))$",
        "Support": "[a,b]",
        "Special Notes": "Basic continuous distribution",
        "Relationships": "Standardized → U(0,1)"
      },
      {
        "group": "common",
        "Name": "Exponential",
        "Type": "Continuous",
        "Parameters": "\\lambda > 0",
        "PMF/PDF": "$f(x) = \\lambda e^{-\\lambda x}, x\\ge0$",
        "CDF": "$F(x) = 1 - e^{-\\lambda x}$",
        "E[X]": "$E[X] = 1/\\lambda$",
        "Var(X)": "$Var(X) = 1/\\lambda^2$",
        "When to Use": "Time between Poisson events",
        "MGF": "$M_X(t) = \\frac{\\lambda}{\\lambda-t}, t<\\lambda$",
        "Support": "[0,∞)",
        "Special Notes": "Memoryless property",
        "Relationships": "Gamma(1,λ), Poisson process"
      },
      {
        "group": "more complex",
        "Name": "Gamma",
        "Type": "Continuous",
        "Parameters": "α>0, β>0",
        "PMF/PDF": "$f(x) = x^{α-1} e^{-x/β}/(β^α Γ(α)), x≥0$",
        "CDF": "Incomplete gamma function",
        "E[X]": "$E[X] = α β$",
        "Var(X)": "$Var(X) = α β^2$",
        "When to Use": "Sum of exponentials; waiting times",
        "MGF": "$M_X(t) = (1-β t)^{-α}, t<1/β$",
        "Support": "[0,∞)",
        "Special Notes": "Generalizes Exponential",
        "Relationships": "Chi-Square = Gamma(k/2,2)"
      },
      {
        "group": "more complex",
        "Name": "Beta",
        "Type": "Continuous",
        "Parameters": "α>0, β>0",
        "PMF/PDF": "$f(x) = x^{α-1} (1-x)^{β-1}/B(α,β), 0≤x≤1$",
        "CDF": "Incomplete beta function",
        "E[X]": "$E[X] = α/(α+β)$",
        "Var(X)": "$Var(X) = αβ/((α+β)^2 (α+β+1))$",
        "When to Use": "Modeling probabilities",
        "MGF": "No simple form",
        "Support": "[0,1]",
        "Special Notes": "Flexible shapes",
        "Relationships": "Conjugate prior for Binomial"
      },
      {
        "group": "common",
        "Name": "Normal",
        "Type": "Continuous",
        "Parameters": "μ, σ>0",
        "PMF/PDF": "$f(x) = \\frac{1}{\\sqrt{2\\pi}σ} e^{-\\frac{(x-μ)^2}{2σ^2}}$",
        "CDF": "Φ((x-μ)/σ), standard normal CDF",
        "E[X]": "$E[X] = μ$",
        "Var(X)": "$Var(X) = σ^2$",
        "When to Use": "Many natural phenomena; Central Limit Theorem",
        "MGF": "$M_X(t) = e^{μt + σ^2 t^2/2}$",
        "Support": "(-∞,∞)",
        "Special Notes": "Bell-shaped curve",
        "Relationships": "Sum of Normals → Normal"
      },
      {
        "group": "more complex",
        "Name": "Lognormal",
        "Type": "Continuous",
        "Parameters": "μ, σ>0",
        "PMF/PDF": "$f(x) = \\frac{1}{x σ \\sqrt{2\\pi}} e^{-\\frac{(\\ln x - μ)^2}{2σ^2}}, x>0$",
        "CDF": "$F(x) = Φ((\\ln x - μ)/σ)$",
        "E[X]": "$E[X] = e^{μ+σ^2/2}$",
        "Var(X)": "$Var(X) = (e^{σ^2}-1)e^{2μ+σ^2}$",
        "When to Use": "Multiplicative processes, stock prices",
        "MGF": "No simple form",
        "Support": "(0,∞)",
        "Special Notes": "Skewed distribution",
        "Relationships": "ln(X) ~ Normal"
      },
      {
        "group": "more complex",
        "Name": "Chi-Square",
        "Type": "Continuous",
        "Parameters": "k>0",
        "PMF/PDF": "$f(x) = x^{k/2-1} e^{-x/2}/(2^{k/2} Γ(k/2)), x≥0$",
        "CDF": "Incomplete gamma function",
        "E[X]": "$E[X] = k$",
        "Var(X)": "$Var(X) = 2k$",
        "When to Use": "Sum of squares of standard normals",
        "MGF": "$M_X(t) = (1-2t)^{-k/2}$",
        "Support": "[0,∞)",
        "Special Notes": "Used in hypothesis testing",
        "Relationships": "Special case of Gamma"
      },
      {
        "group": "more complex",
        "Name": "t",
        "Type": "Continuous",
        "Parameters": "ν>0",
        "PMF/PDF": "$f(x) = \\frac{Γ((ν+1)/2)}{\\sqrt{νπ} Γ(ν/2)} (1+x^2/ν)^{-(ν+1)/2}$",
        "CDF": "Student t CDF",
        "E[X]": "$E[X] = 0, ν>1$",
        "Var(X)": "$Var(X) = ν/(ν-2), ν>2$",
        "When to Use": "Small sample inference",
        "MGF": "No simple form",
        "Support": "(-∞,∞)",
        "Special Notes": "Heavy tails compared to Normal",
        "Relationships": "t² → F(1,ν)"
      },
      {
        "group": "more complex",
        "Name": "F",
        "Type": "Continuous",
        "Parameters": "d1>0, d2>0",
        "PMF/PDF": "$f(x) = \\frac{d1^{d1/2} d2^{d2/2}}{B(d1/2,d2/2)} \\frac{x^{d1/2-1}}{(d2+d1 x)^{(d1+d2)/2}}, x>0$",
        "CDF": "F-distribution CDF",
        "E[X]": "$E[X] = d2/(d2-2), d2>2$",
        "Var(X)": "$Var(X) = [2 d2^2 (d1+d2-2)]/[d1(d2-2)^2(d2-4)], d2>4$",
        "When to Use": "Ratio of scaled Chi-Square variables",
        "MGF": "No simple form",
        "Support": "(0,∞)",
        "Special Notes": "Used in ANOVA and F-tests",
        "Relationships": "t² = F(1,ν)"
      },
      {
        "group": "more complex",
        "Name": "Weibull",
        "Type": "Continuous",
        "Parameters": "k>0, λ>0",
        "PMF/PDF": "$f(x) = \\frac{k}{λ} (x/λ)^{k-1} e^{-(x/λ)^k}, x≥0$",
        "CDF": "$F(x) = 1 - e^{-(x/λ)^k}$",
        "E[X]": "$E[X] = λ Γ(1+1/k)$",
        "Var(X)": "$Var(X) = λ^2 (Γ(1+2/k) - (Γ(1+1/k))^2)$",
        "When to Use": "Modeling life data, reliability",
        "MGF": "No simple form",
        "Support": "[0,∞)",
        "Special Notes": "Generalizes Exponential",
        "Relationships": "Exponential = Weibull(k=1)"
      }
    ]
  }
  